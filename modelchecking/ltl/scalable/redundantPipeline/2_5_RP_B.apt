.name "redundantFlow_basis"
.type LPN
.options
nb_switches="9"

.places
in[yCoord=510.0, xCoord=450.0]
out[yCoord=510.0, xCoord=2130.0, reach="true"]
p0[yCoord=390.0, xCoord=750.0]
p1[yCoord=390.0, xCoord=1050.0]
p2[yCoord=630.0, xCoord=690.0]
p3[yCoord=630.0, xCoord=1050.0]
p4[yCoord=630.0, xCoord=1290.0]
p5[yCoord=630.0, xCoord=1530.0]
p6[yCoord=630.0, xCoord=1830.0]

.transitions
createFlows[label="createFlows", yCoord=510.0, xCoord=270.0, tfl="in -> {in},> -> {in}"]
tD0[label="tD0", weakFair="true", yCoord=630.0, xCoord=570.0, tfl="p2 -> {p2},in -> {p2}"]
tD1[label="tD1", weakFair="true", yCoord=630.0, xCoord=870.0, tfl="p2 -> {p3},p3 -> {p3}"]
tD2[label="tD2", weakFair="true", yCoord=630.0, xCoord=1170.0, tfl="p3 -> {p4},p4 -> {p4}"]
tD3[label="tD3", weakFair="true", yCoord=630.0, xCoord=1410.0, tfl="p4 -> {p5},p5 -> {p5}"]
tD4[label="tD4", weakFair="true", yCoord=630.0, xCoord=1650.0, tfl="p5 -> {p6},p6 -> {p6}"]
tDout[label="tDout", weakFair="true", yCoord=630.0, xCoord=1950.0, tfl="p6 -> {out},out -> {out}"]
tU0[label="tU0", weakFair="true", yCoord=390.0, xCoord=570.0, tfl="p0 -> {p0},in -> {p0}"]
tU1[label="tU1", weakFair="true", yCoord=390.0, xCoord=870.0, tfl="p0 -> {p1},p1 -> {p1}"]
tUout[label="tUout", weakFair="true", yCoord=390.0, xCoord=1950.0, tfl="p1 -> {out},out -> {out}"]

.flows
createFlows: {1*in} -> {1*in}
tD0: {1*p2, 1*in} -> {1*in, 1*p2}
tD1: {1*p3, 1*p2} -> {1*p3, 1*p2}
tD2: {1*p4, 1*p3} -> {1*p4, 1*p3}
tD3: {1*p4, 1*p5} -> {1*p4, 1*p5}
tD4: {1*p5, 1*p6} -> {1*p6, 1*p5}
tDout: {1*out, 1*p6} -> {1*p6, 1*out}
tU0: {1*p0, 1*in} -> {1*in, 1*p0}
tU1: {1*p1, 1*p0} -> {1*p0, 1*p1}
tUout: {1*out, 1*p1} -> {1*out, 1*p1}

.initial_marking {1*in, 1*out, 1*p0, 1*p1, 1*p2, 1*p3, 1*p4, 1*p5, 1*p6}
