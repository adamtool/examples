LB[yCoord=750.0, xCoord=390.0, env="true"]
SA[yCoord=210.0, xCoord=390.0, token=1]
SAA[yCoord=30.0, xCoord=750.0, token=1]
SB[yCoord=570.0, xCoord=390.0, token=2]
